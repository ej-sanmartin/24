{
  "extends": [
    "next/core-web-vitals",
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": ["error", { "code": 80, "ignoreUrls": true, "ignoreStrings": true, "ignoreTemplateLiterals": true }],
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "indent": ["error", 2],
    "object-curly-spacing": ["error", "never"]
  }
}